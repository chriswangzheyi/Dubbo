<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://code.alibabatech.com/schema/dubbo 
		http://code.alibabatech.com/schema/dubbo/dubbo.xsd
		http://www.springframework.org/schema/beans  
		http://www.springframework.org/schema/beans/spring-beans.xsd 
		http://www.springframework.org/schema/context 
	 	http://www.springframework.org/schema/context/spring-context-4.1.xsd">
	 	
	 	
	<!-- 定义要进行Annotation扫描包名称 --> 
	<context:component-scan base-package="com.wzy.*" />
	<!-- 设置本文件加载的时候要加载的其他配置文件 -->
	<import resource="classpath:META-INF/spring/spring-dubbo-consumer.xml"/>
	
</beans>
